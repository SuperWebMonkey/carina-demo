<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Carina Demo Tests - API Sample" parallel="methods" thread-count="5">

    <parameter name="suiteOwner" value="qpsdemo"/>

    <test name="API tests">
        <classes>
            <class name="com.qaprosoft.carina.demo.APISampleTest">
                <methods>
                    <include name="testDeleteEmployees"/>
                    <include name="testGetEmployee"/>
                    <include name="testCreateEmployee"/>
                    <include name="testCreateEmployeeMissingSomeFields"/>
                    <include name="testPutEmployee"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>